<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Barn2Door Home Page">
		<parameter name="URL" value ="barn2door.com"/>
		<test name="Home Page Testing">
 
    <classes>
      <class name="classesPackage.Parent"/>
      <class name="classesPackage.Login"/>
      		<methods>
      			<exclude name = "MobileLoginHome"/>
      			<exclude name ="Mobile.*"/>
      			
      		</methods>
      		
      <class name="classesPackage.A"/>
      <methods>
      			<include name = "OnlyRunMe"/>
      </methods>
      
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->







